programa
{
	funcao inicio()
	{
		real  matriz[3][2]
		inteiro indice 

		escreva("Entre com os valores da 1ยบ linha:\n")
		para(indice = 0; indice < 3; indice++)
		{
			leia(matriz[indice][0])
		}
		escreva("Entre com os valores da 2ยบ linha:\n")
		para(indice = 0; indice < 3; indice++)
		{
			leia(matriz[indice][1])
		}
	}
}
